run = "python3 main.py"
modules = ["python-3.11"]
hidden = [".config"]

[gitHubImport]
requiredFiles = [".replit", "replit.nix", "main.py"]

[languages]
[languages.python3]
pattern = "**/*.py"

[languages.python3.languageServer]
start = "pylsp"

[nix]
channel = "stable-22_11"

[env]
PYTHONPATH = "."
FLASK_ENV = "development"
